{
    "name": "EE",
    "version": "1.0.0",
    "description": "to track employees knowledge levels and specialties",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "debug": "cross-env NODE_ENV=dev nodemon --inspect ./index.js",
        "dev": "cross-env NODE_ENV=dev node ./index.js",
        "build": "webpack -w --progress --config webpack.config.dev.js",
        "build:prod": "del-cli dist && ngc -p tsconfig.aot.json && ngc -p tsconfig.aot.json && webpack --config webpack.config.prod.js --progress --profile --bail && del-cli 'src/app/**/*.js' 'src/app/**/*.ngfactory.ts' 'src/app/**/*.js.map' 'src/app/**/*.shim.ts' 'src/app/**/*.ngsummary.json' 'src/app/**/*.ngstyle.ts' 'dist/app'"
    },
    "author": "John Birkeland",
    "repository": "https://github.com/rbirkeland80/employee_expertise.git",
    "license": "ISC",
    "nodemonConfig": {
        "ignore": [
            "client/*",
            "dist/*",
            "node_modules/*",
            "testData/*",
            ".gitignore",
            "package.json",
            "webpack.*"
        ],
        "delay": "2500"
    },
    "dependencies": {
        "@angular/animations": "^4.0.1",
        "@angular/common": "^4.0.1",
        "@angular/compiler": "^4.0.1",
        "@angular/compiler-cli": "^4.0.1",
        "@angular/core": "^4.0.1",
        "@angular/forms": "^4.0.1",
        "@angular/http": "^4.0.1",
        "@angular/platform-browser": "^4.0.1",
        "@angular/platform-browser-dynamic": "^4.0.1",
        "@angular/platform-server": "^4.0.1",
        "@angular/router": "^4.0.1",
        "@angular/upgrade": "^4.0.1",
        "bcrypt-nodejs": "0.0.3",
        "body-parser": "^1.18.2",
        "core-js": "^2.4.1",
        "express": "^4.16.2",
        "express-session": "^1.15.6",
        "jade": "^1.11.0",
        "mongoose": "^4.13.1",
        "passport": "^0.4.0",
        "passport-local": "^1.0.0",
        "rxjs": "^5.3.0",
        "zone.js": "^0.8.5"
    },
    "devDependencies": {
        "@types/core-js": "0.9.36",
        "@types/node": "^6.0.45",
        "angular-router-loader": "^0.6.0",
        "angular2-template-loader": "^0.6.2",
        "awesome-typescript-loader": "^3.1.2",
        "cross-env": "^5.1.3",
        "del-cli": "^0.2.1",
        "html-loader": "^0.4.5",
        "html-webpack-plugin": "^2.28.0",
        "lite-server": "^2.3.0",
        "node-sass": "^4.7.2",
        "nodemon": "^1.14.11",
        "raw-loader": "^0.5.1",
        "sass-loader": "^6.0.6",
        "typescript": "^2.2.2",
        "webpack": "^2.3.3",
        "webpack-dev-middleware": "^2.0.4",
        "webpack-dev-server": "^2.4.2",
        "webpack-hot-middleware": "^2.21.0",
        "webpack-merge": "^4.1.0"
    }
}
